{
  "name": "lookup-repo",
  "version": "0.1.0",
  "description": "This is a project to practice using the MERN stack",
  "private": true,
  "main": "server.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gh-pages": "^3.1.0",
    "mongoose": "^5.12.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "NPM_CONFIG_PRODUCTION=false npm install && node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:react": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b production -d build",
    "dev": "nodemon -r dotenv/config server.js"
  },
  "proxy": "http://localhost:3000",

  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
